<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_registracija_fizicki"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.coky.app.RegistracijaFizickiKorisnik">

    <TextView
        android:text="Email:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_alignBaseline="@+id/editMailRF"
        android:layout_alignBottom="@+id/editMailRF"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:layout_below="@+id/editPonovljenaRF"
        android:layout_alignStart="@+id/editPonovljenaRF"
        android:id="@+id/editOibRF" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_below="@+id/editOibRF"
        android:layout_alignStart="@+id/editOibRF"
        android:id="@+id/editGradRF" />

    <TextView
        android:text="Lozinka:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_alignBaseline="@+id/editLozinkaRF"
        android:layout_alignBottom="@+id/editLozinkaRF"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="OIB:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:layout_alignBaseline="@+id/editOibRF"
        android:layout_alignBottom="@+id/editOibRF"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Grad:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:layout_alignBaseline="@+id/editGradRF"
        android:layout_alignBottom="@+id/editGradRF"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Adresa:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_alignBaseline="@+id/editAdresaRF"
        android:layout_alignBottom="@+id/editAdresaRF"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_below="@+id/editKontaktRF"
        android:layout_alignEnd="@+id/editKontaktRF"
        android:id="@+id/editImeRF" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editAdresaRF"
        android:layout_below="@+id/editGradRF"
        android:layout_alignStart="@+id/editGradRF" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editKontaktRF"
        android:layout_below="@+id/editAdresaRF"
        android:layout_alignStart="@+id/editAdresaRF" />

    <TextView
        android:text="Ime:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_alignBaseline="@+id/editImeRF"
        android:layout_alignBottom="@+id/editImeRF"
        android:layout_alignParentStart="true" />



    <TextView
        android:text="Kontakt:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:layout_alignBaseline="@+id/editKontaktRF"
        android:layout_alignBottom="@+id/editKontaktRF"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/editMailRF"
        android:inputType="textEmailAddress"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="26dp" />

    <TextView
        android:text="Lozinka (Ponovljena):"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_above="@+id/editOibRF"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editLozinkaRF"
        android:layout_toStartOf="@+id/editOibRF" />

    <Button
        android:text="Odustani"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonOdustaniRF"
        android:layout_alignBaseline="@+id/buttonRegistrirajSeRF"
        android:layout_alignBottom="@+id/buttonRegistrirajSeRF"
        android:layout_toEndOf="@+id/buttonRegistrirajSeRF"
        android:layout_marginStart="32dp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editLozinkaRF"
        android:layout_below="@+id/editMailRF"
        android:layout_alignStart="@+id/editMailRF" />

    <TextView
        android:text="Prezime:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:id="@+id/textView3"
        android:layout_below="@+id/editImeRF"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:layout_below="@+id/editImeRF"
        android:layout_alignStart="@+id/editImeRF"
        android:id="@+id/editPrezimeRF" />

    <Button
        android:text="Registriraj se"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:id="@+id/buttonRegistrirajSeRF"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/textView10" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editPonovljenaRF"
        android:layout_below="@+id/editLozinkaRF"
        android:layout_alignStart="@+id/editLozinkaRF" />
</RelativeLayout>
